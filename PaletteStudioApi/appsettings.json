{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "JwtSettings": {
    "Issuer": "PaletteStudioApi",
    "Audience": "PaletteStudioApiClient",
    "DurationDays": 1,
    "Key": "b7051ba8-a740-43fa-bde0-f4e3f12c8266-ThisIsForDemoPurposesOnly"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PaletteStudioDbContext": "Filename=palleteStudio.db",
    "ApiIdentityDbContext": "Filename=apiIdentity.db"
  }
}